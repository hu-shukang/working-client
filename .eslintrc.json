{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["@nuxtjs/eslint-config-typescript", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "import/no-named-as-default-member": "off",
    "object-shorthand": "off"
  }
}
